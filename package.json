{
  "name": "sqreen-reader",
  "productName": "sQReen Reader",
  "version": "2.0.5",
  "description": "sQReen Reader is a desktop tool that watches your screen for QR codes and gives you a way to open the hyperlinks embedded in the QR code.",
  "main": "src/index.js",
  "scripts": {
    "test": "nyc --reporter=lcov --reporter=html --reporter=text mocha",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --publish onTagOrDraft",
    "release": "electron-builder",
    "lint": "eslint .",
    "test:e2e": "mocha e2e"
  },
  "keywords": [],
  "author": {
    "name": "matthew-js-porter",
    "email": "matthew.js.porter@jpmchase.com"
  },
  "license": "MIT",
  "build": {
    "appId": "net.mporter",
    "mac": {
      "category": "public.app-category.utilities",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist"
    },
    "afterSign": "scripts/notarize.js"
  },
  "dependencies": {
    "electron-log": "^4.3.4",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^4.3.8",
    "jpeg-js": "^0.4.3",
    "jsqr": "^1.3.1",
    "qrcode": "^1.4.4"
  },
  "devDependencies": {
    "chai": "4.3.4",
    "dotenv": "10.0.0",
    "electron": "13.2.1",
    "electron-builder": "22.11.7",
    "electron-notarize": "1.1.0",
    "eslint": "7.32.0",
    "eslint-config-google": "0.14.0",
    "mocha": "9.1.0",
    "nyc": "15.1.0",
    "proxyquire": "2.1.3",
    "sinon": "11.1.2",
    "spectron": "15.0.0"
  }
}
