<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.image.ImageView?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="net.sqreenreader.controller.SqreenReaderController">
    <Label fx:id="noQRCode" text="No Recent QR Codes"/>
    <Hyperlink fx:id="latestQRCode" visible="false"/>
    <ImageView fx:id="qrCodeImage"
               fitHeight="200" fitWidth="200.0"
               layoutX="69.0" layoutY="322.0"
               pickOnBounds="true"
               preserveRatio="true"
               visible="false"/>
</VBox>